  {
    "args": [
      "run",
      "lint"
    ],
    "command": "npm",
    "retry_on_failure": false,
    "task_type": "Lint"
  },
  {
    "args": [
      "test"
    ],
    "command": "npm",
    "retry_on_failure": true,
    "task_type": "Test"
  },
  {
    "args": [
      "build"
    ],
    "command": "webpack",
    "retry_on_failure": false,
    "task_type": "Build"
  },
  {
    "args": [
      "apply",
      "-f",
      "deployment.yaml"
    ],
    "command": "kubectl",
    "retry_on_failure": false,
    "task_type": "Deploy"
  }
]